# Server Configuration
PORT=3000
NODE_ENV=development

# Serial Port Configuration
SERIAL_BAUD_RATE=115200
SERIAL_DATA_BITS=8
SERIAL_STOP_BITS=1
SERIAL_PARITY=none

# Device Settings
DEFAULT_CUTTING_SPEED=50
DEFAULT_PRESSURE=300
MAX_CUTTING_WIDTH=600
MAX_CUTTING_HEIGHT=1200

# File Upload
MAX_FILE_SIZE=10485760
UPLOAD_DIR=./uploads

# Database
DB_PATH=./database/thundercut.db

# Logging
LOG_LEVEL=info
LOG_DIR=./logs
